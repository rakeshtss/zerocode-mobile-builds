{
    "name": "com.ocoa.zerocode",
    "displayName": "ocoa",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "figer": "keytool -list -v -keystore keys/default_zc_mobile.jks -alias tss-key -storepass The@1234 -keypass",
        "key": "keytool -list -v -keystore keys/default_zc_mobile.jks -alias tss-key -storepass The@1234 -keypass The@1234"
    },
    "keywords": [
        "ecosystem:cordova"
    ],
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "devDependencies": {
        "cordova-android": "^12.0.1",
        "cordova-plugin-geolocation": "^5.0.0",
        "cordova-plugin-inappbrowser": "^5.0.0",
        "cordova-plugin-request-location-accuracy": "^2.3.0",
        "cordova-plugin-statusbar": "^3.0.0"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-inappbrowser": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-geolocation": {
                "GPS_REQUIRED": "true"
            },
            "cordova-plugin-request-location-accuracy": {
                "PLAY_SERVICES_LOCATION_VERSION": "16.+"
            }
        },
        "platforms": [
            "android"
        ]
    }
}